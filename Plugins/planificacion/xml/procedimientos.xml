<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('procedimientos_id_seq')</default>
    </column>
    <column>
        <name>subproceso_id</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>varchar(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>descripcion</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>procedimientos_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>fk_procedimientos_subprocesos</name>
        <type>FOREIGN KEY (subproceso_id) REFERENCES subprocesos (id) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
</table>